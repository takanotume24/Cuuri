{
  "imports": {
    "@tauri-apps/api/core": "npm:@tauri-apps/api/core",
    "vite-plugin-vue-devtools": "npm:vite-plugin-vue-devtools",
    "@vitejs/plugin-vue": "npm:@vitejs/plugin-vue",
    "@deno/vite-plugin": "npm:@deno/vite-plugin",
    "vite": "npm:vite",
    "vue": "npm:vue",
    "bootstrap": "npm:bootstrap"
  },
  "tasks": {
    "dev": "deno run -A npm:vite",
    "build": "deno check src/main.ts && deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "tauri": "deno run -A npm:@tauri-apps/cli"
  },
  "nodeModulesDir": "auto"
}
